		"LSstorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LSstorage'"
		},
		"LS_SQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "riteshsqlserver.database.windows.net"
		},
		"LS_SQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "riteshsqldb"
